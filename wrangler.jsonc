{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "www-worker",
  "main": "src/worker.js",
  "compatibility_date": "2025-09-05",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
 "vars": {
    "NO_COLOR": true
  },
 "assets": {
		"binding": "ASSETS",
		"directory": ".open-next/assets"
	},
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}
